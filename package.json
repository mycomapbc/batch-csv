{
  "name": "mycomap-batch-validator",
  "version": "1.0.0",
  "description": "This is a helper script to validate a spreadsheet containing 2 columns of data: the BC Mycomap voucher number. This is specifically for the BC Mycomap project, which has the `BCXX-` prefix.",
  "author": "Ben Keen",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "build": "npm run webpackProd",
    "start": "npm run webpackDev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "webpackDev": "webpack serve --config ./webpack.config.js --mode=development",
    "webpackProd": "webpack --config ./webpack.config.js --mode=production",
    "update-gh-pages": "npm run build && node build/gh-pages.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benkeen/mycomap-batch-validator.git"
  },
  "bugs": {
    "url": "https://github.com/benkeen/mycomap-batch-validator/issues"
  },
  "homepage": "https://github.com/benkeen/mycomap-batch-validator#readme",
